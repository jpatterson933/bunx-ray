{
  "name": "bunx-ray",
  "version": "1.2.1",
  "description": "ASCII heat-map bundle viewer for webpack, vite, and esbuild",
  "license": "MIT",
  "author": "jpatterson933",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpatterson933/bunx-ray.git"
  },
  "keywords": [
    "bundle",
    "treemap",
    "webpack",
    "vite",
    "esbuild",
    "bundle-analyzer",
    "ascii",
    "cli",
    "bundle-size"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "bunx-ray": "dist/cli.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "execa": "^9.6.1",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
